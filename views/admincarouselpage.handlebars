                                <td>{{this.poster_name}}</td>
                                <td>{{this.poster_link}}</td>
                                <td>{{this.created_by}}</td>
                                <td>{{this.created_on}}</td> 
                                <td class="text-center">
                                    <i style="color:red" class="fa fa-trash" onclick="trash('{{this.poster_name}}')"></i>
                                </td>                               
                            </tr>
                        {{/each}}
