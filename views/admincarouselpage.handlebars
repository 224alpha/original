                        <tr>
                            <th>SLIDER NAME</th>
                            <th>SLIDER LINK</th>
                            <th>CREATED BY</th>
                            <th>CREATED ON</th>
                            <th>OPERATIONS</th>
                        </tr>
                    </thead>
